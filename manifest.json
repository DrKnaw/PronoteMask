{
    "name": "PronoteMask",
    "version": "2.0",
    "description": "Retire automatiquement vos informations personnelles sur votre espace Pronote.",
    "icons": {
        "128": "img/logo.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*/pronote/eleve*"],
            "js": ["mask.js"]
        }
    ],
"host_permissions": [
  "*://*/pronote/eleve*"
],
    "manifest_version": 3
  }