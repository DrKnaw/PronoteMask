{
    "name": "PronoteMask",
    "version": "1.1",
    "description": "Retire automatiquement vos informations personnelles sur votre espace Pronote.",
    "permissions": ["activeTab"],
    "icons": {
        "128": "img/logo.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*/pronote/eleve*"],
            "js": ["mask.js"]
        }
    ],
    "manifest_version": 2
  }